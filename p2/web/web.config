			<add name="MyHttpModule" type="MyHttpModule"/>
		</httpModules>
		<webServices>
			<protocols>
				<add name="HttpGet"/>
				<add name="HttpPost"/>
			</protocols>
		</webServices>
		<compilation debug="true"/></system.web>
</configuration>
