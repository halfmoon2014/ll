<configuration>
	<appSettings>
		<add key="DBCon" value="Data Source=192.168.1.204,1330;Initial Catalog=sq_mysys;User ID=dblogin;Password=Dblogin123456!;"/>
    <!-- jquery easy ui Themes-->
    <add key="Themes" value="pepper-grinder"/>
