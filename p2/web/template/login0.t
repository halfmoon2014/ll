<html>
<head>
    <title>$title$</title>       
    $css$        
    $javascripts$
</head>
<body>
    <div>
        <table class="table1">
            <tr>
                <td>
                    <table class="table2">
                        <tr>
                            <td class="td_empty">
                            </td>
                            <td class="td_content">
                                <table class="table3">
                                    <tr>
                                        <td class="table3_logo">
                                        </td>
                                        <td class="table3_content">
                                            <table class="tm1_top">
                                                <tr>
                                                    <td class="tm1_empty">
                                                        &nbsp;
                                                    </td>
                                                    <td class="text_right">
                                                        用户名
                                                    </td>
                                                    <td class="text_left">
                                                        <input type="text" id="usr" class="easyui-validatebox" data-options="required:true,missingMessage:'用户名不能为空'"
                                                            style='width: 140px; height: 22px;' />
                                                    </td>
                                                    <td>
                                                        &nbsp;
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td class="tm1_empty">
                                                        &nbsp;
                                                    </td>
                                                    <td class="text_right">
                                                        密码
                                                    </td>
                                                    <td class="text_left">
                                                        <input type="password" id="psw" class="easyui-validatebox" data-options="required:true,missingMessage:'密码能为空'"
                                                            style='width: 140px; height: 22px;' />
                                                    </td>
                                                    <td>
                                                        &nbsp;
                                                    </td>
                                                </tr>
                                            </table>
                                            <table class="tm2_top">
                                                <tr align="center">
                                                    <td class="tm2_empty">
                                                        &nbsp;
                                                    </td>
                                                    <td align="center">
                                                        <a href="javascript:void(0)" class="easyui-linkbutton" id="ok">确定</a>
                                                    </td>
                                                    <td align="center">
                                                        <a href="javascript:void(0)" class="easyui-linkbutton" id="esc">清除</a>
                                                    </td>
                                                </tr>
                                            </table>
                                        </td>
                                    </tr>
                                </table>
                            </td>
                            <td class="td_empty">
                            </td>
                        </tr>
                    </table>
                </td>
            </tr>
        </table>
    </div>
</body>
</html>
